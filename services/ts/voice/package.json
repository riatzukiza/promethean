{
    "name": "voice-service",
    "version": "0.0.1",
    "type": "module",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "start": "node dist/index.js",
        "test": "cross-env TS_NODE_TRANSPILE_ONLY=1 ava tests/dummy.test.ts",
        "coverage": "npm run build && c8 ava \"dist/tests/**/*.js\""
    },
    "dependencies": {
        "@discordjs/voice": "^0.18.0",
        "discord.js": "^14.17.3",
        "express": "^4.19.2",
        "prism-media": "^1.3.5",
        "wav": "^1.0.2"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/node": "^22.10.10",
        "ava": "^6.4.1",
        "ts-node": "^10.9.2",
        "cross-env": "^10.0.0",
        "typescript": "5.7.3",
        "c8": "^9.1.0"
    },
    "ava": {
        "extensions": {
            "ts": "module"
        },
        "files": [
            "tests/**/*.ts"
        ],
        "nodeArguments": [
            "--loader",
            "ts-node/esm"
        ]
    }
}
